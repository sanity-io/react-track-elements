{
  "name": "@sanity/react-track-elements",
  "version": "1.0.0",
  "description": "Keep track of values provided by child elements deeply nested in a React component tree",
  "main": "dist/cjs/index.js",
  "scripts": {
    "start": "snowpack dev",
    "build": "npm run build:es2015 && npm run build:esm && npm run build:cjs",
    "build:es2015": "tsc --declaration --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --declaration --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --declaration --module commonjs --target es5 --outDir dist/cjs",
    "clean": "rimraf dist",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "es2015": "dist/es2015/index.js",
  "keywords": [
    "track",
    "overlay",
    "react",
    "deep",
    "deeply",
    "tree",
    "report"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "nano-pubsub": "^2.0.0",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@juggle/resize-observer": "^3.2.0",
    "@snowpack/plugin-babel": "^2.1.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.14",
    "@types/lodash-es": "^4.17.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/shallowequal": "^1.1.1",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "shallowequal": "^1.1.0",
    "snowpack": "^2.14.1",
    "styled-components": "^5.2.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
